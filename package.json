{
  "name": "scout",
  "displayName": "Scout",
  "version": "0.0.1",
  "description": "Agents filling google forms",
  "author": "marban",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.2.17",
    "ai": "^4.3.15",
    "framer-motion": "^12.10.4",
    "plasmo": "^0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "prettier": "3.2.4",
    "tailwindcss": "3",
    "typescript": "5.3.3"
  },
  "manifest": {
    "permissions": [
      "storage",
      "activeTab",
      "scripting",
      "tabs",
      "sidePanel"
    ],
    "host_permissions": [
      "https://*.google.com/forms/*",
      "https://*.forms.office.com/*",
      "https://*.microsoft.com/forms/*",
      "https://*.docs.google.com/forms/*"
    ]
  }
}
